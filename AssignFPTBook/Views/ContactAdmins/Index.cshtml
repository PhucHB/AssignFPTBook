@model IEnumerable<ContactAdmin>

<h1>...................</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>

            <th scope="col">Name of Contact</th>
            <th scope="col">Description of Contact</th>
            <th scope="col">User ID </th>
            <th scope="col">Status</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.DecriptionContact</td>
                <td>@item.UserId</td>
                <td>@item.Status</td>
                <td>
                    <a asp-controller="ContactAdmins"
                   asp-action="Edit"
                   asp-route-id=@item.Id
                   class="btn btn-info">
                        Edit
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
